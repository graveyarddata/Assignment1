{
  "steps": [
    { "name": "gcr.io/cloud-builders/gsutil",
      "args": ["cp","gs://${_CONF_REPO}/parameters.json","./data/parameters.json"] },

    { "name": "${_LOCATION}-docker.pkg.dev/$PROJECT_ID/${_REPOSITORY}/pipeline-executor:0.0.1",
      "args": [
        "--pipeline_spec_path=./pipeline/penguins_pipeline.yaml",
        "--project=$PROJECT_ID",
        "--region=us-central1",
        "--pipeline_root=gs://${_TEMP_REPO}",
        "--parameters=./data/parameters.json"
      ]}
  ],
  "options": { "logging": "CLOUD_LOGGING_ONLY" }
}
